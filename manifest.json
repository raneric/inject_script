{
  "name": "IDPLien",
  "description": "Handle IDPLien",
  "version": "1.0",
  "permissions": [
    "contextMenus",
    "activeTab",
    "clipboardRead",
    "notifications",
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "idplien.js"
    ]
  },
  "icons": {
    "16": "injection.png",
    "48": "injection.png",
    "128": "injection.png"
  },
  "commands": {
    "fill_link": {
      "suggested_key": {
        "default": "Ctrl+Shift+3"
      },
      "description": "open all link in new tab",
      "global": true
    },
	 "open_link": {
      "suggested_key": {
        "default": "Ctrl+Shift+1"
      },
      "description": "open all link in new tab",
      "global": true
    }
  },
  "browser_action": {},
  "options_page": "menu.html",
  "manifest_version": 2
}